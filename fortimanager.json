        {
            "action": "create firewall policy",
            "description": "Create an ADOM firewall policy",
            "type": "investigate",
            "identifier": "create_firewall_policy",
            "read_only": true,
            "parameters": {
                "level": {
                    "description": "Level type at which to create the firewall policy. Choosing 'ADOM' (Administrative Domain Name) of the FortiManager server allows you to perform against that particular ADOM",
                    "data_type": "string",
                    "required": true,
                    "value_list": [
                        "ADOM"
                    ],
                    "default": "ADOM",
                    "order": 0
                },
                "adom": {
                    "description": "ADOM name",
                    "data_type": "string",
                    "order": 1
                },
                "package": {
                    "description": "The Policy Package name or full folder path to create the firewall policy within",
                    "data_type": "string",
                    "required": true,
                    "default": "default",
                    "order": 2
                },
                "name": {
                    "description": "Policy name to create",
                    "data_type": "string",
                    "required": true,
                    "order": 3
                },
                "source_interface": {
                    "description": "Incoming (ingress) interface to specify for the firewall policy",
                    "data_type": "string",
                    "default": "any",
                    "required": true,
                    "order": 4
                },
                "destination_interface": {
                    "description": "Outgoing (egress) interface to specify for the firewall policy",
                    "data_type": "string",
                    "default": "any",
                    "required": true,
                    "order": 5
                },
                "source_address": {
                    "description": "Source IPv4 Address and address group names to specify for the firewall policy",
                    "data_type": "string",
                    "default": "all",
                    "required": true,
                    "order": 6
                },
                "destination_address": {
                    "description": "Destination IPv4 Address and address group names to specify for the firewall policy",
                    "data_type": "string",
                    "default": "all",
                    "required": true,
                    "order": 7
                },
                "action": {
                    "description": "Policy action to specify for the firewall policy. Accept: Allows sessions that match the firewall policy. Deny: Blocks sessions that match the firewall policy. IPSec: Firewall policy becomes a policy-based IPsec VPN policy",
                    "data_type": "string",
                    "required": true,
                    "default": "deny",
                    "value_list": [
                        "deny",
                        "accept",
                        "ipsec"
                    ],
                    "order": 8
                },
                "status": {
                    "description": "'Enable' or 'Disable' this firewall policy on your FortiManager instance",
                    "data_type": "string",
                    "required": true,
                    "default": "enable",
                    "value_list": [
                        "enable",
                        "disable"
                    ],
                    "order": 9
                },
                "schedule": {
                    "description": "Name for the schedule to be associated with the firewall policy (e.g. always, none)",
                    "data_type": "string",
                    "default": "always",
                    "required": true,
                    "order": 10
                },
                "service": {
                    "description": "Service and Service group names to create for the firewall policy",
                    "data_type": "string",
                    "default": "ALL",
                    "required": true,
                    "order": 11
                },
                "inspection_mode": {
                    "description": "Firewall policy Inspection Mode",
                    "data_type": "string",
                    "required": true,
                    "default": "flow",
                    "value_list": [
                        "proxy",
                        "flow"
                    ],
                    "order": 12
                },
                "log_traffic": {
                    "description": "Enables or disables logging of either all sessions or only security profile sessions",
                    "data_type": "string",
                    "required": true,
                    "default": "disable",
                    "value_list": [
                        "all",
                        "utm",
                        "disable"
                    ],
                    "order": 13
                }
            },
